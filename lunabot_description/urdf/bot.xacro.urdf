  </link>

  <joint name="left_front_rev" type="continuous">
    <origin rpy="0 0 0" xyz="-0.292 -0.35 -0.162"/>
    <parent link="base_link"/>
    <child link="left_front"/>
    <axis xyz="0 1 0"/>
    <!-- <limit effort="10.0" velocity="100.0"/> -->
