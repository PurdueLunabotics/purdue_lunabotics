        <!-- <box size="0.9 0.5 0.1"/> -->
        <mesh filename="model://realistic_robot/realistic_robot.stl"/>
      </geometry>
      <material name="silver"/>
    </visual>

    <collision>
      <origin rpy="3.14 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="model://realistic_robot/realistic_robot.stl"/>
      </geometry>
    </collision>
  </link>

  <!-- left front -->
  <link name="left_front">
    <inertial>
      <origin xyz="0 0 0"/>
      <mass value="5.0"/>
      <inertia  ixx="0.056510417" ixy = "0" ixz = "0"
