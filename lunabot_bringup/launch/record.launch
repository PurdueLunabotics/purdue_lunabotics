<?xml version="1.0"?>
<launch>
  <exec cmd="mkdir /bags" output="screen"/>
  <arg name="exp_name" default="exp" />
  <node name="recorder" pkg="rosbag" type="record"
    args="--repeat-latched -o /bags/$(var exp_name) 

  /goal /maps/costmap_node/global_costmap/costmap /nav/global_path 
  
  /cmd_vel /rtabmap/odom /tf /tf_static /effort /sensors /errors /imu 

  /d455_back/camera/color/image_rect_color /d455_back/camera/color/tag_detections 
  /d455_back/camera/aligned_depth_to_color/image_raw /d455_back/camera/aligned_depth_to_color/camera_info 
  /d455_back/camera/color/camera_info /d455_back/camera/points 

  /d455_front/camera/color/image_rect_color /d455_front/camera/color/tag_detections
  /d455_front/camera/aligned_depth_to_color/image_raw /d455_front/camera/aligned_depth_to_color/camera_info 
  /d455_front/camera/color/camera_info /d455_front/camera/points 

  /d435_left/camera/color/image_rect_color /d435_left/camera/color/tag_detections
  /d435_left/camera/color/camera_info

  /d435_right/camera/color/image_rect_color /d435_right/camera/color/tag_detections
  /d435_right/camera/color/camera_info
  
  /pointcloud/depth_combined_cloud
  /cloud_map

  /usb_cam/tag_detections /usb_cam/image_color 

  /mining_zone /apriltag_visual /berm_zone
  
  /ptp/log /ptp/target_pose /ptp/current_target /ptp/robot_state 
  /ptp/line_path /ptp/angular_disparity /ptp/linear_disparity /ptp/heading /ptp/angle_target /ptp/pid_linear /ptp/pid_angular

  /purePursuit/controller_vel /purePursuit/target_vel /purePursuit/slip_vec /purePursuit/final_vel /purePursuit/heading_err
  "
    output="screen" />
</launch>
